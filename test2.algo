PROCEDURE Dot product(u,v)
VAR
 u : ARRAY_OF FLOAT[2];
 v : ARRAY_OF FLOAT [2];
 i: INTEGER :=0 ;
 ps : FLOAT :=0

BEGIN
FOR i FROM 0 TO u.length-1 STEP 1  DO
 ps:=p+(u[i]*p[i])
END_FOR
write (ps)
 END
 FUNCTION Fdot product(u,v) : FLOAT
 VAR
  u : ARRAY_OF FLOAT[2];
  v : ARRAY_OF FLOAT [2];
  i: INTEGER :=0 ;
  ps : FLOAT :=0
 BEGIN
    FOR i FROM 0 TO u.length-1 STEP 1  DO
  ps:=p+(u[i]*p[i]) 
     RETURN ps ;
 END

 ALGORITHM produit scalaire
